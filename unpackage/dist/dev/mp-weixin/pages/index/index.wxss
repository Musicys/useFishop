@-webkit-keyframes rotate {
from {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
to {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes rotate {
from {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
to {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.opacity {
  opacity: .6;
}
.fade-to {
  -webkit-animation: fade-to 0.5s;
          animation: fade-to 0.5s;
}
@-webkit-keyframes fade-to {
from {
    height: 0;
}
to {
    height: 40%;
}
}
@keyframes fade-to {
from {
    height: 0;
}
to {
    height: 40%;
}
}

