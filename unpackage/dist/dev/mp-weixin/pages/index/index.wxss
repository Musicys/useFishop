.index {
  width: 750rpx;
  height: 100vh;
  position: relative;
}
/* /index_top */
.index_top {
  height: 40%;
  width: 750rpx;
  background: #FFFFFF;
  position: relative;
}
.index_top image {
  width: 100%;
  height: 110%;
  position: absolute;
  top: -150rpx;
  z-index: 0;
}
.index_top .top_bt {
  z-index: 2;
  top: 30%;
  position: absolute;
  color: #FFFFFF;
  padding-left: 50rpx;
}
.index_top .top_bt > text:nth-child(1) {
  font-weight: bold;
  font-size: 56rpx;
  display: block;
}
.index_but {
  width: 750rpx;
  height: 70%;
  bottom: 0;
  position: absolute;
  border-radius: 25rpx 25rpx 0rpx 0rpx;
  overflow: hidden;
}
.index_but .scrollview {
  width: 100%;
  height: 100%;
}
.index_but .scrollview .top {
  width: 100%;
  background: ghostwhite;
  height: 49%;
  background: #F6F6F6;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.index_but .scrollview .top .up_top {
  display: flex;
}
.index_but .scrollview .top .up_top view {
  border-radius: 20rpx;
}
.index_but .scrollview .top .up_top .buttonone {
  background: linear-gradient(to left, #5392F2, #427DF1);
  width: 300rpx;
  height: 300rpx;
  margin-right: 20rpx;
  position: relative;
}
.index_but .scrollview .top .up_top .buttonone text:nth-child(1) {
  margin-top: 30rpx;
  font-weight: bold;
  color: #E0EBFC;
}
.index_but .scrollview .top .up_top .buttonone text:nth-child(2) {
  margin-top: 5rpx;
  color: #B9D1F9;
  font-size: 25rpx;
}
.index_but .scrollview .top .up_top .buttonone text {
  padding-left: 30rpx;
  display: block;
}
.index_but .scrollview .top .up_top .up_top_right {
  display: flex;
  flex-direction: column;
  width: 300rpx;
  height: 300rpx;
}
.index_but .scrollview .top .up_top .up_top_right .buttontwo {
  margin-bottom: 10rpx;
  flex: 1;
  background: #58BE6A;
  color: #E0EBFC;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-weight: bold;
}
.index_but .scrollview .top .up_top .up_top_right .buttontwo text {
  font-size: .8em;
}
.index_but .scrollview .top .up_top .up_top_right .buttonthree {
  border-top: 10rpx;
  flex: 1;
  margin-top: 2rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-weight: bold;
}
.index_but .scrollview .top .up_top .up_top_right .buttonthree text {
  font-size: .8em;
}
.index_but .scrollview .top .up_but {
  display: flex;
  background: #EFEFEF;
  margin-top: 10rpx;
  border-radius: 20rpx;
  width: 620rpx;
  justify-content: space-between;
}
.index_but .scrollview .top .up_but view {
  width: 100%;
  text-align: center;
  height: 75rpx;
  border-radius: 20rpx;
  background: #FFFFFF;
  margin: 10rpx 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #D6D9DE;
  font-weight: bold;
}
.index_but .scrollview .but {
  width: 100%;
  background: #F7F7F7;
}
.index_but .scrollview .but .bt {
  display: block;
  margin-left: 8%;
  color: #1B232E;
  padding-top: 2%;
  font-weight: bold;
  height: 8%;
}
.index_but .scrollview .but .fishCart {
  height: 90%;
  width: 750rpx;
}
